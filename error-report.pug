doctype html
html
  head
    meta(charset='utf-8')
    title Upload
  body
    h2 Generated on #{form.fields['X-Amz-Date']}
    form(action=form.action, method='POST', enctype='multipart/form-data')
      each value, field in form.fields
        input(type='hidden', name=field.toLowerCase(), value=value)
      input(type='file', name=form.filefield)
      input(type='submit', value='Save')
